    private static void configureProbes(Model model) {
        String[][] probeConfig = {
            {"dom1", "P11", "-solid.PxX"},
            {"dom2", "P21", "-solid.PyX"},
            {"dom3", "P31", "-solid.PzX"},
            {"dom4", "P12", "-solid.PxY"},
            {"dom5", "P22", "-solid.PyY"},
            {"dom6", "P32", "-solid.PzY"},
            {"dom7", "P13", "-solid.PxZ"},
            {"dom8", "P23", "-solid.PyZ"},
            {"dom9", "P33", "-solid.PzZ"}
